<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nikfen.trafficdevilstestapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".model.TDApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TrafficDevilsTestApplication">
        <activity
            android:name=".view.game.TriplePole"
            android:exported="false" />
        <activity
            android:name=".view.game.DoublePole"
            android:exported="false" />
        <activity
            android:name=".view.web.WebActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:exported="false" />
        <activity
            android:name=".view.game.GameActivity"
            android:exported="false" />
        <activity
            android:name=".view.FOpenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>